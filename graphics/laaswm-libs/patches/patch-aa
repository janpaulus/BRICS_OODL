# Use system tclsh instead of eltclsh

--- Makefile.in.orig	2009-03-09 12:15:24.000000000 +0100
+++ Makefile.in	2009-03-09 13:43:33.191187000 +0100
@@ -703,7 +703,7 @@
 
 install-data-hook:
 	rm -f $(DESTDIR)$(tcldir)/pkgIndex.tcl
-	eltclsh $(srcdir)/mkIndex $(DESTDIR)$(tcldir)
+	tclsh $(srcdir)/mkIndex $(DESTDIR)$(tcldir)
 # Tell versions [3.59,3.63) of GNU make to not export all variables.
 # Otherwise a system limit (for SysV at least) may be exceeded.
